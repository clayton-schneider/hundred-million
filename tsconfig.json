{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@data/*": [
        "src/data/*"
      ],
      "@img/*": [
        "src/img/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@sections/*": [
        "src/sections/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}